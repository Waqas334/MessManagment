<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_meal">

    <fragment
        android:id="@+id/navigation_meal"
        android:name="com.androidbull.messmanagment.ui.meal.HomeFragment"
        android:label="@string/title_meal"
        tools:layout="@layout/fragment_meal" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.androidbull.messmanagment.ui.menu.MenuFragment"
        android:label="@string/title_menu"
        tools:layout="@layout/fragment_menu" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.androidbull.messmanagment.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.androidbull.messmanagment.ui.user.UserFragment"
        android:label="@string/title_user"
        tools:layout="@layout/fragment_user" />
</navigation>